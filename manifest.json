{
  "manifest_version": 3,
  "name": "Kiosk Screensaver",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "idle"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["tat_popup.js", "popu.js"],
      "matches": ["<all_urls>"]
    }
  ]
}