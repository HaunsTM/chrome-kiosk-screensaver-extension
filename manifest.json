{
  "manifest_version": 3,
  "name": "Kiosk Screensaver",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "tabs",
    "idle",
    "storage",

    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://10.0.0.6:8123/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "content.js",
        "documentEventListeners.js"
      ]
    }
  ]
}